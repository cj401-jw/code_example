{
    "arch": "mobilenet_v2",
    "pretrain": 0,
    "n_clas": 200,
    "im_sz": 224,
    "batch_size": 96,
    "bs_test": 16,
    "learning_rate": [
        1e-3,
        1e-3,
        1e-3
    ],
    "wd": 1e-8,
    "ps": 0.5,
    "num_epochs": 5,
    "num_workers": 8,
    "cuda": 1,
    "scheduler": "one_cycle",
    "div_factor": 25,
    "pct_start": 0.1,
    "save_summary_steps": 10,
    "early_stopping": {
        "patience": 10,
        "monitor_metric": "loss",
        "mode": "minimize"
    },
    "hyper_search": {
        "ps": [
            0.5,
            0.75
        ]
    }
}
